# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def removeNthFromEnd(self, head: Optional[ListNode], n: int) -> Optional[ListNode]:
        l=0
        curr=head
        while curr!=None:
            curr=curr.next
            l=l+1
        curr=head
        if n==l:
            return head.next
        else:
            k=l-n
            for i in range(k-1):
                curr=curr.next
            curr.next=curr.next.next
            return head
