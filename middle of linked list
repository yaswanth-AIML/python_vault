# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def middleNode(self, head: Optional[ListNode]) -> Optional[ListNode]:
        cur=head
        length=0
        while cur!=None:
            cur=cur.next
            length=length+1
        cur=head
        for i in range(length//2):
            cur=cur.next
        return cur
#finds the middle of the list 
